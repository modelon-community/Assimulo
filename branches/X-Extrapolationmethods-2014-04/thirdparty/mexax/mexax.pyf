python module mexax__user__routines 
    interface mexax_user_interface 
        subroutine solout(np,nv,nu,nl,t,p,v,u,a,rlam,infos,irtrn) 
            integer, intent(in) :: np
            integer, intent(in) :: nv
            integer, intent(in) :: nu
            intege, intent(in)r :: nl
            double precision, intent(in) :: t
            double precision dimension(np), intent(in) :: p
            double precision dimension(nv), intent(in) :: v
            double precision dimension(nu), intent(in) :: u
            double precision dimension(nv), intent(in) :: a
            double precision dimension(nl), intent(in) :: rlam
            integer dimension(50) :: infos
            integer, intent(in) :: irtrn
        end subroutine solout
        subroutine fprob(np, nv, nl, ng, nu, ldg,  &
                                  t, p, v, u, rlam, &
                                  am, gp, f, pdot, udot, g, gi, fl, &
                                  qflag, ifail) 
            integer :: np
            integer :: nv
            integer :: nl
            integer :: ng
            integer :: nu
            integer, optional,check(shape(gp,0)==ldg),depend(gp) :: ldg=shape(gp,0)
            double precision :: t
            double precision dimension(np) :: p
            double precision dimension(nv) :: v
            double precision dimension(nu) :: u
            double precision dimension(nl) :: rlam
            double precision dimension(nv,nv) :: am
            double precision dimension(ldg,*) :: gp          ! lookup
            double precision dimension(nv) :: f
            double precision dimension(np) :: pdot
            double precision dimension(nu) :: udot
            double precision dimension(ng) :: g
            double precision dimension(nl) :: gi
            double precision dimension(nv,nl) :: fl
            logical dimension(9) :: qflag
            integer :: ifail
        end subroutine fprob
        subroutine denout(np,nv,nu,nl,t, p,v,u,a,rl,info, irtrn)

            integer, intent(in) :: np, nv, nu, nl
            integer dimension(50), intent(in) :: info
            integer, intent(out):: irtrn
            double precision, intent(in) :: t
            double precision dimension(np), intent(in) :: p
            double precision dimension(nv), intent(in) :: v
            double precision dimension(nu), intent(in) :: u
            double precision dimension(nv), intent(in) :: a
            double precision dimension(nl), intent(in) :: rl
        end subroutine denout
        subroutine fswit(np,nv,nl,nu,t,p,v,u,a,rlam,nswif,g) 
            integer, intent(in) :: np
            integer, intent(in) :: nv
            integer, intent(in) :: nl
            integer, intent(in) :: nu
            double precision :: t
            double precision dimension(np) :: p
            double precision dimension(nv) :: v
            double precision dimension(nu) :: u
            double precision dimension(nu) :: a
            double precision dimension(nl) :: rlam
            integer   :: nswif
            double precision dimension(nswif) :: g
        end subroutine fswit
    end interface mexax_user_interface 
end python module mexax__user__routines
python module mexax ! in 
